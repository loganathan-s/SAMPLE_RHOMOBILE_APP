<div data-role="page" data-add-back-btn="false">

  <div data-role="header" data-position="inline">
    <h1><%= @developer.developer_id %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="arrow-l" data-direction="reverse">
      Back
    </a>
    <a href="<%= url_for :action => :edit, :id => @developer.object %>" class="ui-btn-right" data-icon="star">
      Edit
    </a>
  </div>

  <div data-role="content">
    <ul data-role="listview">
      
          <li>
            <div class="itemLabel">Id</div>
            <div class="itemValue"><%= @developer.developer_id %></div>
          </li>
      
          <li>
            <div class="itemLabel">FirstName</div>
            <div class="itemValue"><%= @developer.first_name %></div>
          </li>
      
          <li>
            <div class="itemLabel">LastName</div>
            <div class="itemValue"><%= @developer.last_name %></div>
          </li>
      
          <li>
            <div class="itemLabel">Email</div>
            <div class="itemValue"><%= @developer.email %></div>
          </li>
      
          <li>
            <div class="itemLabel">UserName</div>
            <div class="itemValue"><%= @developer.user_name %></div>
          </li>
      
          <li>
            <div class="itemLabel">Status</div>
            <div class="itemValue"><%= @developer.status %></div>
          </li>
      
          <li>
            <div class="itemLabel">Locale</div>
            <div class="itemValue"><%= @developer.locale %></div>
          </li>
      
          <li>
            <div class="itemLabel">Organization</div>
            <div class="itemValue"><%= @developer.organization %></div>
          </li>
      
          <li>
            <div class="itemLabel">Address</div>
            <div class="itemValue"><%= @developer.address %></div>
          </li>
      
          <li>
            <div class="itemLabel">SupportContact</div>
            <div class="itemValue"><%= @developer.support_contact %></div>
          </li>
      
          <li>
            <div class="itemLabel">SupportPhone</div>
            <div class="itemValue"><%= @developer.support_phone %></div>
          </li>
      
    </ul>
  </div>

</div>
