<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Developer</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @developer.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Developer: </td>
                <td class="itemValue"><input type="text" name="developer[developer_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">First name: </td>
                <td class="itemValue"><input type="text" name="developer[first_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Last name: </td>
                <td class="itemValue"><input type="text" name="developer[last_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Email: </td>
                <td class="itemValue"><input type="text" name="developer[email]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">User name: </td>
                <td class="itemValue"><input type="text" name="developer[user_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Status: </td>
                <td class="itemValue"><input type="text" name="developer[status]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Locale: </td>
                <td class="itemValue"><input type="text" name="developer[locale]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Organization: </td>
                <td class="itemValue"><input type="text" name="developer[organization]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Address: </td>
                <td class="itemValue"><input type="text" name="developer[address]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Support contact: </td>
                <td class="itemValue"><input type="text" name="developer[support_contact]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Support phone: </td>
                <td class="itemValue"><input type="text" name="developer[support_phone]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
